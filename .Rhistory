RMSEA.test = rmsea(ncp.test,df)
if(criterion=="BIC"){
return_val = 1/bic
}else if(criterion=="RMSEA"){
return_val= -RMSEA.test
}
if(method=="GA"){
return(return_val + 1)
}else if(method=="tabuSearch"){
return(return_val + 1)
}
}
}
