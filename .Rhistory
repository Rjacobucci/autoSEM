}
for(i in 1:length(jjj)){
if(sum(jjj[[i]]) < minInd){
if(method=="GA"){
return(-99999999)
}else if(method=="tabuSearch"){
return(0)
}
}
}
ooo = list()
for(i in 1:nfac){
facc = paste("f",i,sep="")
ooo[[i]] = paste(paste(facc," =~ "), paste(lll[[i]][jjj[[i]]==1], collapse= "+"))
}
fmld <- ""
for(jj in 1:nfac){
fmld <- paste(fmld,ooo[[jj]],sep="\n")
}
p = length(unique(unlist(varList)))
